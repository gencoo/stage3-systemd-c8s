CC="icc"
CXX="icpc"
CFLAGS="-m64 -march=native -mtune=native -O2 -pipe -fomit-frame-pointer -fno-alias -fPIC -ipo -finline-functions -fno-math-errno -fno-trapping-math"
CXXFLAGS="$CFLAGS"
FCFLAGS="${CFLAGS}"
FFLAGS="${CFLAGS}"
LDFLAGS="-Wl,-O2,--sort-common,--as-needed,--hash-style=gnu"
AR="xiar"
LD="xild"
